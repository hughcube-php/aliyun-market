{
    "name": "hughcube/aliyun-market",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "hugh.li",
            "email": "hugh.li@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "hughcube/guzzle-http": "*",
        "hughcube/laravel-service-support": "*"
    },
    "require-dev": {
        "laravel/framework": "*",
        "laravel/lumen-framework": "*",
        "squizlabs/php_codesniffer": "*",
        "phpstan/phpstan": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4": {
            "HughCube\\Laravel\\AliYunMarket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HughCube\\Laravel\\AliYunMarket\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HughCube\\Laravel\\AliYunMarket\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse -vvv",
        "check-style": "phpcs -p --standard=PSR2 src/ -v",
        "fix-style": "phpcbf -p --standard=PSR2 src/ -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
